<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xzg56.jg.modules.common.persistence.dao.TruckFrameDao">

    <select id="get" resultType="TruckFrame">
        SELECT * FROM mst_truck_frame WHERE id = #{id}
    </select>

    <select id="getByFrameCardNo" resultType="TruckFrame">
        SELECT  * FROM mst_truck_frame WHERE frame_card_id = #{frameCardNo}
    </select>
</mapper>